{
  "name": "ham2k-polo",
  "version": "0.4.7",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "run:android:dev": "react-native run-android --tasks installDevDebug --appIdSuffix=dev",
    "run:android:beta": "react-native run-android --tasks installBetaDebug --appIdSuffix=prod",
    "run:android:prod": "react-native run-android --tasks installProdDebug --appIdSuffix=prod",
    "build:android:dev:release": "cd android && ./gradlew bundleDevRelease && open app/build/outputs/bundle/devRelease",
    "build:android:beta:release": "cd android && ./gradlew bundleBetaRelease && open app/build/outputs/bundle/betaRelease",
    "build:android:prod:release": "cd android && ./gradlew bundleProdRelease && open app/build/outputs/bundle/prodRelease",
    "run:ios:dev": "react-native run-ios --scheme polo-Dev --configuration DevDebug",
    "run:ios:beta": "react-native run-ios --scheme polo-Beta --configuration BetaDebug",
    "run:ios:prod": "react-native run-ios --scheme polo-Prod --configuration ProdDebug",
    "build:ios:dev:release": "cd ios && xcodebuild -scheme polo-Dev build",
    "build:ios:beta:release": "cd ios && xcodebuild -scheme polo-Beta build",
    "build:ios:prod:release": "cd ios && xcodebuild -scheme polo-Prod build"
  },
  "dependencies": {
    "@developerblue/react-native-paper-dropdown": "github:ham2k/react-native-paper-dropdown",
    "@ham2k/lib-callsigns": "^0.1",
    "@ham2k/lib-country-files": "^0.9",
    "@ham2k/lib-dxcc-data": "^0.2",
    "@ham2k/lib-format-tools": "^0.3",
    "@ham2k/lib-operation-data": "^0.1.1",
    "@ham2k/lib-qson-tools": "^0.4",
    "@react-native-async-storage/async-storage": "^1.19.8",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^1.9.7",
    "auto-bind": "^5.0.1",
    "clone-deep": "^4.0.1",
    "color": "^4.2.3",
    "debounce": "^2.0.0",
    "deepmerge": "^4.3.1",
    "fast-xml-parser": "^4.3.2",
    "fuzzball": "^2.1.2",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-blob-util": "^0.19.6",
    "react-native-csv": "^0.2.0",
    "react-native-document-picker": "^9.1.0",
    "react-native-drawer-layout": "^3.2.2",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-pager-view": "^6.2.3",
    "react-native-paper": "^5.11.3",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-share": "^10.0.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-sqlite-2": "^3.6.2",
    "react-native-uuid": "^2.0.1",
    "react-native-vector-icons": "^10.0.2",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.1.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "react-native-flipper": "^0.164.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2"
  },
  "engines": {
    "node": ">=16"
  }
}
